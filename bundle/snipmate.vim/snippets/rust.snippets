snippet if
	if ${1:/* condition */} {
		${2:/* code */}
	}
snippet else
	else {
		${1:/* code */}
	}
snippet fn
	fn ${1:main}(${2:param}) ${3:-> retval} {
		${4:/* code */}
	}
snippet re
	return
snippet match
	match ${1:x} {
		${2:x => code}
	}
snippet for
	for ${1:var} in ${2:expr} {
		${3: /* code */ }
	}
snippet wh
	while ${1:/* condition */} {
		${2:/* code */}
	}
snippet loop
	loop {
		${1:/* code */}
	}
